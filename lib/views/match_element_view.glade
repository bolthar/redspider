<?xml version="1.0"?>
<interface>
  <requires lib="gtk+" version="2.16"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GtkHBox" id="container">
    <property name="width_request">330</property>
    <property name="height_request">25</property>
    <property name="visible">True</property>
    <property name="spacing">5</property>
    <child>
      <object class="GtkLabel" id="score">
        <property name="width_request">30</property>
        <property name="visible">True</property>
        <property name="label" translatable="yes">&lt;0&gt;</property>
      </object>
      <packing>
        <property name="expand">False</property>
        <property name="position">0</property>
      </packing>
    </child>
    <child>
      <object class="GtkButton" id="email">
        <property name="label" translatable="yes">button</property>
        <property name="width_request">150</property>
        <property name="visible">True</property>
        <property name="can_focus">True</property>
        <property name="receives_default">True</property>
        <property name="xalign">0</property>
        <signal name="clicked" handler="on_email_clicked"/>
      </object>
      <packing>
        <property name="expand">False</property>
        <property name="position">1</property>
      </packing>
    </child>
  </object>
</interface>
